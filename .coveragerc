# Coverage.py配置文件

[run]
source = .
omit =
    */venv/*
    */env/*
    */migrations/*
    */settings/*
    */tests/*
    manage.py
    */node_modules/*
    */static/*
    */media/*
    */staticfiles/*
    */__pycache__/*
    */.*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Huali Edu Test Coverage Report

[xml]
output = coverage.xml
